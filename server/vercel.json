{
    "version": 2,
    "name": "E-Bazar",
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "functions": {
        "api/index.js": {
            "memory": 256,
            "maxDuration": 10
        }
    },
    "includeFiles": [
        "server/models/**/*.js",
        "server/controllers/**/*.js",
        "server/utils/**/*.js"
    ]
}
